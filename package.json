{
  "name": "speed-logger",
  "version": "2.0.0",
  "description": "Log network speed to a CSV routinely. ",
  "main": "dist/server/speed.js",
  "scripts": {
    "watch-server": "tsc --watch --project ./src/server/tsconfig.json",
    "compile-server": "tsc --project ./src/server/tsconfig.json",
    "build-server": "run-s clean-server compile-server",
    "clean-server": "rimraf dist/server",
    "watch-client": "tsc --esModuleInterop ./src/client/webpack/webpack.dev.ts && webpack --config ./src/client/webpack/webpack.dev.js --watch",
    "compile-client": "tsc --esModuleInterop ./src/client/webpack/webpack.prod.ts && webpack --config ./src/client/webpack/webpack.prod.js",
    "build-client": "run-s clean-client compile-client",
    "clean-client": "rimraf dist/client",
    "watch-node": "nodemon --watch ./dist/server",
    "watch-node-web": "nodemon --watch ./dist/server -- --type=web",
    "build": "run-s build-server build-client",
    "start": "node dist/server/speed.js",
    "lint": "eslint --fix --ext .ts,.tsx src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericmann/speed-logger.git"
  },
  "keywords": [
    "speed"
  ],
  "author": "Eric Mann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericmann/speed-logger/issues"
  },
  "homepage": "https://github.com/ericmann/speed-logger#readme",
  "browserslist": [
    "> 1%",
    "last 3 version"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "apexcharts": "^3.22.2",
    "chart.js": "^2.9.4",
    "chartjs-plugin-streaming": "^1.8.0",
    "dayjs": "^1.9.6",
    "delay": "^4.4.0",
    "express": "^4.17.1",
    "normalize.css": "^8.0.1",
    "react": "^16.14.0",
    "react-apexcharts": "^1.3.7",
    "react-chartjs-2": "^2.10.0",
    "react-dom": "^16.13.1",
    "socket.io": "^3.0.1",
    "socket.io-client": "^3.0.1",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.27",
    "@types/d3-scale": "^3.2.0",
    "@types/d3-shape": "^2.0.0",
    "@types/delay": "^3.1.0",
    "@types/express": "^4.17.9",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.8",
    "@types/socket.io-client": "^1.4.34",
    "@types/webpack": "^4.41.22",
    "@types/yargs": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.7.0",
    "autoprefixer": "^10.0.2",
    "css-loader": "^4.3.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.13.0",
    "mini-css-extract-plugin": "^1.0.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.7",
    "postcss-loader": "^4.0.4",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.3",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.0.0",
    "webpack-merge": "^5.4.0"
  }
}
